            <stringProp name="Argument.name">DemoRpm</stringProp>
            <stringProp name="Argument.value"></stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
            <stringProp name="Argument.desc">Total throughtput for Demo Requests.</stringProp>
          </elementProp>
